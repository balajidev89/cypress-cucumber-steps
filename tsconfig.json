{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "dom"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "types": ["cypress", "node"]
  },
  "include": ["src"]
}
